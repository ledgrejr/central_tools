[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=mgresham
Group=www-data
WorkingDirectory=/home/mgresham/central_tools
ExecStart=/usr/local/bin/gunicorn --access-logfile - --timeout 250  --workers 5 --bind unix:/home/mgresham/central_tools/core/core.sock core.wsgi:application

[Install]
WantedBy=multi-user.target
